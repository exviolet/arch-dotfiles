{
  "context_servers": {
    "mcp-server-context7": {
      "enabled": true,
      "settings": {
        "context7_api_key": "ctx7sk-9635348d-873b-4977-a235-e1a837eb2c01"
      }
    }
  },
  "agent": {
    "default_profile": "write",
    "default_model": {
      "provider": "google",
      "model": "gemini-3-pro-preview"
    },
    "model_parameters": []
  },
  "title_bar": {
    "show_menus": false,
    "show_branch_icon": true
  },
  "theme": "Moegi Dark",
  "project_panel": {
    "dock": "right",
    "auto_fold_dirs": false
  },
  "inlay_hints": {
    "enabled": false
  },
  "base_keymap": "VSCode",
  "vim_mode": true,
  "relative_line_numbers": "enabled",
  "gutter": {
    "folds": false,
    "breakpoints": false,
    "runnables": false
  },
  "minimap": {
    "show": "never"
  },
  "icon_theme": {
    "mode": "dark",
    "light": "Zed (Default)",
    "dark": "Catppuccin Macchiato"
  },
  "ui_font_size": 16,
  "buffer_font_size": 15.0,
  "outline_panel": {
    "button": false
  },
  "collaboration_panel": {
    "button": true
  },
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "tabs": {
    "file_icons": true,
    "git_status": true
  },
  "languages": {
    "Python": {
      "language_servers": ["pyright", "ty", "!basedpyright"]
    },
    "CSS": {
      "language_servers": ["tailwindcss-language-server"]
    }
  },
  "lsp": {
    "pyright": {
      "settings": {
        "python": {
          "pythonPath": ".venv/bin/python"
        }
      }
    },
    "vscode-css-language-server": {
      "settings": {
        "css": {
          "lint": {
            "unknownAtRules": "ignore"
          }
        }
      }
    },
    "basedpyright": {
      "settings": {
        "analysis": {
          "diagnosticMode": "workspace",
          "inlayHints.callArgumentNames": false
        }
      }
    }
  }
}
